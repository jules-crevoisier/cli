{
    "name": "<%= projectName %>/<%= projectName %>",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^<%= versions.laravel %>"<% if (modules.includes('auth')) { %>,
        "tymon/jwt-auth": "^2"<% } %><% if (modules.includes('api-docs')) { %>,
        "darkaonline/l5-swagger": "^8"<% } %><% if (modules.includes('file-upload')) { %>,
        "league/flysystem-aws-s3-v3": "^3"<% } %><% if (databases.includes('mongodb')) { %>,
        "mongodb/laravel-mongodb": "^4"<% } %><% if (databases.includes('redis')) { %>,
        "predis/predis": "^2"<% } %>
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
