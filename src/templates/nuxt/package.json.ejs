{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"<% if (eslintPrettier) { %>,
    "lint": "eslint .",
    "format": "prettier --write ."<% } %><% if (orm === 'prisma') { %>,
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio"<% } %>
  },
  "dependencies": {
    "nuxt": "^<%= versions.nuxt %>",
    "vue": "^3.5",
    "vue-router": "^4"<% if (modules.includes('i18n')) { %>,
    "@nuxtjs/i18n": "^9"<% } %><% if (modules.includes('auth')) { %>,
    "jsonwebtoken": "^9",
    "bcryptjs": "^2"<% } %><% if (orm !== 'prisma' && databases.includes('postgresql')) { %>,
    "pg": "^8"<% } %><% if (orm !== 'prisma' && databases.includes('mysql')) { %>,
    "mysql2": "^3"<% } %><% if (orm !== 'prisma' && databases.includes('sqlite')) { %>,
    "better-sqlite3": "^11"<% } %><% if (databases.includes('mongodb')) { %>,
    "mongodb": "^6"<% } %><% if (databases.includes('redis')) { %>,
    "redis": "^4"<% } %><% if (orm === 'prisma') { %>,
    "@prisma/client": "^6"<% } %><% if (modules.includes('file-upload')) { %>,
    "@aws-sdk/client-s3": "^3",
    "@aws-sdk/s3-request-presigner": "^3"<% } %><% if (modules.includes('email')) { %>,
    "nodemailer": "^6"<% } %><% if (modules.includes('api-docs')) { %>,
    "swagger-jsdoc": "^6",
    "swagger-ui-dist": "^5"<% } %>
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6",
    "tailwindcss": "^<%= versions.tailwind %>"<% if (typescript) { %>,
    "typescript": "^<%= versions.typescript %>"<% } %><% if (eslintPrettier) { %>,
    "@eslint/js": "^<%= versions.eslint %>",
    "eslint": "^<%= versions.eslint %>",
    "eslint-config-prettier": "^10",
    "eslint-plugin-vue": "^9",
    "vue-eslint-parser": "^9",<% if (typescript) { %>
    "typescript-eslint": "^8",<% } %>
    "prettier": "^<%= versions.prettier %>"<% } %><% if (orm === 'prisma') { %>,
    "prisma": "^6",
    "tsx": "^4"<% } %><% if (typescript && orm !== 'prisma' && databases.includes('sqlite')) { %>,
    "@types/better-sqlite3": "^7"<% } %><% if (typescript && orm !== 'prisma' && databases.includes('postgresql')) { %>,
    "@types/pg": "^8"<% } %><% if (modules.includes('auth')) { %>,
    "@types/jsonwebtoken": "^9",
    "@types/bcryptjs": "^2"<% } %><% if (modules.includes('email')) { %>,
    "@types/nodemailer": "^6"<% } %>
  }<% if (orm === 'prisma') { %>,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }<% } %>
}
