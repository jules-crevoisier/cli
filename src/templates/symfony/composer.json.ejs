{
    "name": "<%= projectName %>/<%= projectName %>",
    "type": "project",
    "require": {
        "php": ">=<%= versions.php %>",
        "symfony/framework-bundle": "^<%= versions.symfony %>",
        "symfony/runtime": "^<%= versions.symfony %>",
        "symfony/yaml": "^<%= versions.symfony %>",
        "symfony/dotenv": "^<%= versions.symfony %>"<% if (modules.includes('auth')) { %>,
        "lexik/jwt-authentication-bundle": "^3",
        "symfony/security-bundle": "^<%= versions.symfony %>"<% } %><% if (modules.includes('email')) { %>,
        "symfony/mailer": "^<%= versions.symfony %>"<% } %><% if (modules.includes('api-docs')) { %>,
        "nelmio/api-doc-bundle": "^4"<% } %><% if (modules.includes('i18n')) { %>,
        "symfony/translation": "^<%= versions.symfony %>"<% } %><% if (modules.includes('file-upload')) { %>,
        "league/flysystem-aws-s3-v3": "^3"<% } %><% if (databases.includes('postgresql') || databases.includes('mysql') || databases.includes('sqlite')) { %>,
        "doctrine/orm": "^3",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3"<% } %><% if (databases.includes('mongodb')) { %>,
        "doctrine/mongodb-odm-bundle": "^5"<% } %><% if (databases.includes('redis')) { %>,
        "symfony/cache": "^<%= versions.symfony %>"<% } %>
    },
    "require-dev": {
        "symfony/maker-bundle": "^1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {},
        "post-install-cmd": ["@auto-scripts"],
        "post-update-cmd": ["@auto-scripts"]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
